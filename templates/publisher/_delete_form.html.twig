<form method="post" action="{{ path('app_publisher_delete', {'id': publisher.id}) }}" onsubmit="return confirm('Вы действиетльно хотите удалить этого издателя?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ publisher.id) }}">
    <button class="btn">Удалить</button>
</form>
